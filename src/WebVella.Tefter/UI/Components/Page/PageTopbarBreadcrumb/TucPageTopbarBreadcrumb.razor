@namespace WebVella.Tefter.UI.Components
@inherits TfBaseComponent
<FluentBreadcrumb>
    @foreach (var item in TfAuthLayout.GetState().Breadcrumb)
    {
        <FluentBreadcrumbItem Href="@item.Url">
            @if (item.IconCollapsed is not null)
            {
                <FluentIcon Value="@item.IconCollapsed" Slot="start"/> 
            } 
            @item.Text
        </FluentBreadcrumbItem>
    }
</FluentBreadcrumb>