@inherits FluxorLayout
@layout StateLayout
<TfLayout SidebarExpanded="@SidebarExpanded.Value" Color="var(--tf-admin-color)">
	<Header>
		<TfLayoutHeader>
			<ChildContent>
				<TfSidebarToggle />
				<TfLocation />
			</ChildContent>
			<Aside>
				<TfUserNavigation />
			</Aside>
		</TfLayoutHeader>
	</Header>
	<Navigation>
		<TfLayoutNavigation>
			<ChildContent>
				<TfAdminNavigation />
			</ChildContent>
		</TfLayoutNavigation>
	</Navigation>
	<Body>
		@Body
	</Body>
</TfLayout>

