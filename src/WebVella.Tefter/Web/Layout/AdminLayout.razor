@inherits FluxorLayout
@layout StateLayout
<FluentDesignTheme Mode="@ThemeMode.Value" OfficeColor="@TfConstants.AdminThemeColor" StorageName="@TfConstants.UIThemeLocalKey"/>
<TfLayout SidebarExpanded="@SidebarExpanded.Value" Color="@TfConstants.AdminColor">
	<Header>
		<TfLayoutHeader>
			<ChildContent>
				<TfSidebarToggle />
				<TfLocation />
			</ChildContent>
			<Aside>
				<TfUserNavigation />
			</Aside>
		</TfLayoutHeader>
	</Header>
	<Navigation>
		<TfLayoutNavigation>
			<ChildContent>
				<TfAdminNavigation />
			</ChildContent>
		</TfLayoutNavigation>
	</Navigation>
	<Body>
		@Body
	</Body>
</TfLayout>

