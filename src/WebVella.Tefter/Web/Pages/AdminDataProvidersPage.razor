@namespace WebVella.Tefter.Web.Pages
@page "/admin/data-providers/{*Path}"
@layout AdminLayout
@inherits TfBasePage

@if (ScreenStateSidebarExpanded.Value)
{
	<FluentStack Orientation="Orientation.Vertical" Class="sidebar with-toolbar" Style="@TfConstants.AdminSidebarStyle">
		<TfAdminDataProviderNavigation />
	</FluentStack>
}
<FluentBodyContent Class="content-body with-toolbar">
	<TfSpaceViewToolbar />
	<div class="content-inner">
		<TfSpaceView />
	</div>
</FluentBodyContent>


